<h1>My Tasks</h1>

<div>
    <% @tasks.each do |task| %>
        <input type="checkbox" id="tasks" name="tasks">
        <label for="tasks"><%= link_to(task.title, task_path(task)) %></label>
        <%= button_to 'Destroy task', task_path(task), method: :delete, data: { confirm: 'Are you sure?' } %>
    <% end %>
    
</div>


